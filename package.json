{
  "name": "todo",
  "version": "0.0.0",
  "scripts": {
    "start": "node server-i18n.js",
    "serve": "npm run ng-high-memory -- serve",
    "serve:ssr": "npm run ng-high-memory -- run todo:serve-ssr",
    "serve:pt": "npm run ng-high-memory -- serve --configuration=pt",
    "build": "npm run ng-high-memory -- run todo:prerender",
    "build:stage": "npm run ng-high-memory -- run todo:prerender:stage",
    "build:alpha": "npm run ng-high-memory -- run todo:prerender:alpha",
    "i18n": "npm run ng-high-memory -- extract-i18n",
    "t9n": "npm run ng-high-memory -- run todo:t9n",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:prettier": "prettier --write .",
    "format:eslint": "npm run ng-high-memory -- lint --fix --force",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "npm run ng-high-memory -- lint",
    "test": "npm run ng-high-memory -- test",
    "test:watch": "npm run ng-high-memory -- test --watch",
    "e2e": "cypress run",
    "e2e:interactive": "cypress open",
    "postinstall": "check-node-version --package",
    "ng-high-memory": "node --max_old_space_size=2000 ./node_modules/@angular/cli/bin/ng"
  },
  "private": true,
  "engines": {
    "node": "14.17.0",
    "npm": "7.15.0"
  },
  "dependencies": {
    "@angular/animations": "^12.0.1",
    "@angular/cdk": "^12.0.1",
    "@angular/common": "^12.0.1",
    "@angular/compiler": "^12.0.1",
    "@angular/core": "^12.0.1",
    "@angular/forms": "^12.0.1",
    "@angular/platform-browser": "^12.0.1",
    "@angular/platform-browser-dynamic": "^12.0.1",
    "@angular/platform-server": "^12.0.1",
    "@angular/router": "^12.0.1",
    "@nguniversal/express-engine": "^12.0.0",
    "@lucaspaganini/angular-utils": "^11.0.0",
    "compression": "^1.7.4",
    "connect-slashes": "^1.4.0",
    "express": "^4.15.2",
    "helmet": "^4.6.0",
    "karma-firefox-launcher": "^2.1.0",
    "lodash-es": "^4.17.21",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.1",
    "@angular-eslint/builder": "12.0.0",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/schematics": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "^12.0.1",
    "@angular/compiler-cli": "^12.0.1",
    "@angular/localize": "^12.0.1",
    "@nguniversal/builders": "^12.0.0",
    "@types/compression": "^1.7.0",
    "@types/connect-slashes": "0.0.32",
    "@types/express": "^4.17.11",
    "@types/jasmine": "^3.7.4",
    "@types/jasminewd2": "^2.0.9",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "14.17",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "angular-t9n": "^12.0.0",
    "check-node-version": "^4.1.0",
    "cypress": "^7.4.0",
    "cypress-audit": "^0.3.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsdoc": "^34.8.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "prettier": "^2.3.0",
    "prettier-plugin-jsdoc": "^0.3.22",
    "typescript": "^4.2.4"
  }
}
